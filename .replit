modules = ["python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["freetype", "gumbo", "harfbuzz", "jbig2dec", "lcms2", "libimagequant", "libjpeg", "libjpeg_turbo", "libtiff", "libwebp", "libxcrypt", "mupdf", "openjpeg", "swig", "tcl", "tk", "xcbuild", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "PDF Figure Extractor"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "desktop_app"

[[workflows.workflow]]
name = "PDF Figure Extractor"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install PyMuPDF pillow && python main.py"

[workflows.workflow.metadata]
outputType = "vnc"

[[workflows.workflow]]
name = "desktop_app"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pip install PyMuPDF pillow tkinter && python main.py"
